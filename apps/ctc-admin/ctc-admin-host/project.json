{
  "name": "ctc-admin",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "app",
  "sourceRoot": "apps/ctc-admin/ctc-admin-host/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/angular:webpack-browser",
      "outputs": ["{options.outputPath}"],
      "options": {
        "deployUrl": "/ctc-admin/",
        "outputPath": "dist/apps/ctc-admin/ctc-admin-host",
        "index": "apps/ctc-admin/ctc-admin-host/src/index.html",
        "main": "apps/ctc-admin/ctc-admin-host/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/ctc-admin/ctc-admin-host/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": ["apps/ctc-admin/ctc-admin-host/src/favicon.ico", "apps/ctc-admin/ctc-admin-host/src/assets"],
        "styles": ["apps/ctc-admin/ctc-admin-host/src/styles.scss"],
        "scripts": [],
        "customWebpackConfig": {
          "path": "apps/ctc-admin/ctc-admin-host/webpack.config.ts"
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "customWebpackConfig": {
            "path": "apps/ctc-admin/ctc-admin-host/webpack.prod.config.ts"
          }
        },
        "development": {
          "buildOptimizer": false,
          "optimization": false,
          "vendorChunk": true,
          "extractLicenses": false,
          "sourceMap": true,
          "namedChunks": true
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/angular:module-federation-dev-server",
      "options": {
        "port": 4204,
        "publicHost": "http://cirrusapproach.local:4204",
        "disableHostCheck": true,
        "devRemotes": [
          "header",
          "sidebar",
          "profile-page"
        ]
      },
      "configurations": {
        "production": {
          "buildTarget": "ctc-admin:build:production"
        },
        "development": {
          "buildTarget": "ctc-admin:build:development"
        }
      },
      "defaultConfiguration": "development"
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "ctc-admin:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/ctc-admin/ctc-admin-host/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "ctc-admin:build",
        "port": 4204
      }
    }
  }
}
