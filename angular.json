            "lintFilePatterns": [
              "libs/badge/src/**/*.ts",
              "libs/badge/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "courses": {
      "projectType": "application",
      "root": "apps/courses",
      "sourceRoot": "apps/courses/src",
      "prefix": "cirrus",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:webpack-browser",
          "outputs": ["{options.outputPath}"],
          "options": {
            "deployUrl": "/courses/",
            "outputPath": "dist/apps/courses",
            "index": "apps/courses/src/index.html",
            "main": "apps/courses/src/main.ts",
            "polyfills": "apps/courses/src/polyfills.ts",
            "tsConfig": "apps/courses/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/courses/src/favicon.ico",
              "apps/courses/src/assets",
              {
                "input": "libs/ui/src/assets",
                "glob": "**/*",
                "output": ""
              }
            ],
            "styles": ["apps/courses/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui/src/lib/styles"]
            },
            "scripts": [],
            "customWebpackConfig": {
              "path": "webpack.config.js"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/courses/src/environments/environment.ts",
                  "with": "apps/courses/src/environments/environment.prod.ts"
                },
                {
                  "replace": "apps/courses/src/app/build-specifics/index.ts",
                  "with": "apps/courses/src/app/build-specifics/index.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "staging": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "20kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/courses/src/environments/environment.ts",
                  "with": "apps/courses/src/environments/environment.staging.ts"
                }
              ],
              "outputHashing": "all"
            },
            "uat": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "7kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/courses/src/environments/environment.ts",
                  "with": "apps/courses/src/environments/environment.uat.ts"
                }
              ],
              "outputHashing": "all"
            },
            "review": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": false
              },
              "namedChunks": true,
              "fileReplacements": [
                {
                  "replace": "apps/courses/src/environments/environment.ts",
                  "with": "apps/courses/src/environments/environment.template.ts"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": {
                "scripts": true,
                "styles": true,
                "hidden": false
              },
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@nrwl/angular:webpack-server",
          "configurations": {
            "production": {
              "browserTarget": "courses:build:production"
            },
            "staging": {
              "browserTarget": "courses:build:staging"
            },
            "uat": {
              "browserTarget": "courses:build:uat"
            },
            "development": {
              "host": "cirrusapproach.local",
              "browserTarget": "courses:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "host": "cirrusapproach.local",
            "port": 4201,
            "browserTarget": "courses:build:development"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "courses:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/courses/src/**/*.ts",
              "apps/courses/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/courses"],
          "options": {
            "jestConfig": "apps/courses/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "builder": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/angular",
            "port": 4400,
            "config": {
              "configFolder": "apps/courses/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "builder": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/angular",
            "outputPath": "dist/storybook/courses",
            "config": {
              "configFolder": "apps/courses/.storybook"
            },
            "styles": ["apps/courses/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["libs/ui/src/lib/styles"]
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      },
      "tags": []
    },
    "courses-e2e": {
      "root": "apps/courses-e2e",
      "sourceRoot": "apps/courses-e2e/src",
