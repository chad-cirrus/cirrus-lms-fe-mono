{
  "version": 1,
  "projects": {
    "atc": {
      "root": "apps/atc",
      "sourceRoot": "apps/atc/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:webpack",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/atc",
            "main": "apps/atc/src/main.ts",
            "tsConfig": "apps/atc/tsconfig.app.json",
            "assets": ["apps/atc/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/atc/src/environments/environment.ts",
                  "with": "apps/atc/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:node",
          "options": {
            "buildTarget": "atc:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/atc/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/atc"],
          "options": {
            "jestConfig": "apps/atc/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "badge": {
      "projectType": "library",
      "root": "libs/badge",
      "sourceRoot": "libs/badge/src",
      "prefix": "cirrus",
