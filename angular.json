{
  "version": 1,
  "projects": {
    "atc": {
      "root": "apps/atc",
      "sourceRoot": "apps/atc/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/atc",
            "main": "apps/atc/src/main.ts",
            "tsConfig": "apps/atc/tsconfig.app.json",
            "assets": [
              "apps/atc/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/atc/src/environments/environment.ts",
                  "with": "apps/atc/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "atc:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/atc/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/atc"
          ],
          "options": {
            "jestConfig": "apps/atc/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "courses": {
      "projectType": "application",
      "root": "apps/courses",
      "sourceRoot": "apps/courses/src",
      "prefix": "cirrus",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "baseHref": "/courses/",
            "outputPath": "dist/apps/courses",
            "index": "apps/courses/src/index.html",
            "main": "apps/courses/src/main.ts",
            "polyfills": "apps/courses/src/polyfills.ts",
            "tsConfig": "apps/courses/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/courses/src/favicon.ico",
              "apps/courses/src/assets",
              {
                "input": "libs/ui/src/assets",
                "glob": "**/*",
                "output": "assets/ui"
              }
            ],
            "styles": [
              "apps/courses/src/styles.scss"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/ui/src/lib/styles"
              ]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/courses/src/environments/environment.ts",
                  "with": "apps/courses/src/environments/environment.prod.ts"
                },
                {
                  "replace": "apps/courses/src/app/build-specifics/index.ts",
                  "with": "apps/courses/src/app/build-specifics/index.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "courses:build:production"
            },
            "development": {
              "host": "cirrusapproach.local",
              "browserTarget": "courses:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201,
            "browserTarget": "courses:build:development"
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "courses:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/courses/src/**/*.ts",
              "apps/courses/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/courses"
          ],
          "options": {
            "jestConfig": "apps/courses/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "courses-e2e": {
      "root": "apps/courses-e2e",
      "sourceRoot": "apps/courses-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/courses-e2e/cypress.json",
            "devServerTarget": "courses:serve:development",
            "tsConfig": "apps/courses-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "courses:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/courses-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "courses"
      ]
    },
    "models": {
      "root": "libs/models",
      "sourceRoot": "libs/models/src",
      "projectType": "library",
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/models/**/*.ts"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/models"
          ],
          "options": {
            "jestConfig": "libs/models/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "cirrus",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui"
          ],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/src/**/*.ts",
              "libs/ui/src/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  }
}
